{
	"name": "tasks2do",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "concurrently --names \"Firebase,Next.js\" -c \"blue.bold,yellow.bold\" \"next dev\" \"pnpm run firebase:emulators:start\"",
		"firebase:emulators:start": "firebase emulators:start --only auth,firestore --import=./firebase --export-on-exit",
		"lint": "next lint",
		"prepare": "husky install",
		"prettier": "prettier -w .",
		"start": "next start"
	},
	"dependencies": {
		"autoprefixer": "10.4.14",
		"daisyui": "^2.52.0",
		"firebase": "^9.22.1",
		"next": "13.4.4",
		"postcss": "8.4.24",
		"react": "18.2.0",
		"react-daisyui": "^3.1.2",
		"react-dom": "18.2.0",
		"react-firebase-hooks": "^5.1.1",
		"react-icons": "^4.9.0",
		"tailwindcss": "3.3.2"
	},
	"devDependencies": {
		"@types/node": "20.2.5",
		"@types/react": "18.2.7",
		"@types/react-dom": "18.2.4",
		"concurrently": "^8.0.1",
		"encoding": "^0.1.13",
		"eslint": "8.41.0",
		"eslint-config-next": "13.4.4",
		"husky": "^8.0.3",
		"prettier": "^2.8.8",
		"prettier-quick": "^0.0.5",
		"typescript": "5.0.4"
	}
